{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "outDir": "./public/script",
        "rootDir": "./public/script.ts",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["./public/script.ts/**/*.ts"],
    "exclude": ["node_modules", "**/*.spec.ts"]
}
